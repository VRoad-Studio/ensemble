<div class='d-flex justify-content-between mb-1'>
  <div class='student-name'>
    <% if !work_plan.student.nil? %>
      <h2><strong><%= work_plan.student.first_name%></strong></h2>
    <% end %>
    <%= link_to 'PDF', work_plan_path(@work_plan, format: :pdf)%>
    <%= link_to 'Clone', work_plan_clone_path(@work_plan), method: 'post', class: 'd-none'%>
  </div>

</div>
